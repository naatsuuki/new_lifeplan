<!-- app/views/age_tables/index.html.erb -->
<% content_for :title, '保存された年齢表 - AgeMap' %>

<div class="container">
  <h1>保存された年齢表</h1>

  <ul class="age-table-list">
    <% @age_tables.each do |age_table| %>
      <li>
        <%= age_table.table_title %>
        <%= link_to '削除', age_table_path(age_table), method: :delete, data: { confirm: '本当に削除しますか？' } %>
      </li>
    <% end %>
  </ul>

  <button onclick="history.back()" class="back-button">戻る</button>
</div>
