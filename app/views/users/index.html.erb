<!-- app/views/users/index.html.erb -->
<% content_for :title, 'ユーザー一覧 - AgeMap' %>

<div class="container">
  <h1>ユーザー登録情報</h1>
  <ul>
    <% @users.each do |user| %>
      <li>
        <span><%= user.name %> (<%= user.birth_year %>)</span>
      </li>
    <% end %>
  </ul>
  <button onclick="history.back()" class="back-button">戻る</button>
</div>
